#manual hackery
git clone https://github.com/gmarik/Vundle.vim.git $HOME/.vim/bundle/Vundle.vim

# set up Vundle
git clone https://github.com/gmarik/Vundle.vim.git $HOME/.vim/bundle/Vundle.vim 
vim +PluginInstall +qall

#zsh syntax highlighting
git clone git://github.com/zsh-users/zsh-syntax-highlighting.git $HOME/.zsh-plugins/zsh-syntax-highlighting

#vim text objects
wget https://raw.githubusercontent.com/hchbaw/opp.zsh/master/opp.zsh $HOME/.zsh-plugins/opp.zsh

#vim rust lang
curl https://raw.githubusercontent.com/rust-lang/rust/master/src/etc/vim/syntax/rust.vim > $HOME/.vim/syntax/rust.vim
